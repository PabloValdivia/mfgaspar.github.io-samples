ient.end();\n    });\n    \n    client.subscribe(topic, { qos: 0 });\n\n    client.on(\u0027message\u0027, function (topic, message, packet) {\n        console.log(\u0027Received Message:= \u0027 + message.toString() + \u0027\\nOn topic:= \u0027 + topic);\n\n        var chart = dashboard.getComponentByName(\u0027${c:barChart}\u0027);\n        var lineChart = dashboard.getComponentByName(\u0027${c:lineChart}\u0027);\n        var map = dashboard.getComponentByName(\u0027${c:map}\u0027);\n        var parsedMessage = JSON.parse(message.toString());\n        \n        var msg = message.toString();\n        myself.placeholder().prepend(\u0027\u003Cdiv\u003EDevice ID :\u0027+parsedMessage.measureID+\u0027  - Value: \u0027+parsedMessage.value+\u0027\u003C/div\u003E\u0027);\n\n        \n        chart.chart.datasets[0].bars[Number(parsedMessage.measureID)-1].value = parsedMessage.value;\n        chart.chart.update();\n        \n        var _ = require(\u0027cdf/lib/underscore\u0027);\n        _.each(lineChart.chart.datasets[0].points, function(v, index){\n            var value = (lineChart.chart.datasets[0].points.length\u003C=index+1)?parsedMessage.value:lineChart.chart.datasets[0].points[index+1].value;\n            lineChart.chart.datasets[0].points[index].value = value; \n            lineChart.chart.datasets[0].points[index].label = \u0022\u0022+index+1;\n        });\n        lineChart.chart.update();\n        \n        var lat = Number(parsedMessage.latitude);\n        var long = Number(parsedMessage.longitude);\n        map.mapMarker.setLatLng([lat, long]).update(); \n    });\n\n} "},{"name":"parameters","type":"ValuesArray","value":"[]"},{"name":"manageCallee","type":"Boolean","value":"true"},{"name":"dataSource","type":"Datasource","value":""},{"name":"height","type":"Integer","value":""},{"name":"priority","type":"Integer","value":5},{"name":"width","type":"Integer","value":""},{"name":"htmlObject","type":"HtmlObject","value":"messages"},{"name":"executeAtStart","typ