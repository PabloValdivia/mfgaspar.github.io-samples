r myself=this;\n    var broker = dashboard.getParam(\u0027brokerParam\u0027);\n    var topic = this.dashboard.getParam(\u0027topicParam\u0027);\n\n    var client  = this.client = mqtt.connect(broker);\n  \n    client.on(\u0027error\u0027, function (err) {\n        client.end();\n    });\n    \n    client.on(\u0027connect\u0027, function (err) {\n        client.subscribe(topic, { qos: 0 });\n    });\n\n    client.on(\u0027message\u0027, function (topic, message, packet) {\n        // Log to console\n        // Log.info(\u0027Received Message:= \u0027 + message.toString() + \u0027\\nOn topic:= \u0027 + topic);\n        \n        // Parse message coming from mqtt/websocket\n        var parsedMessage = JSON.parse(message.toString());\n        myself.placeholder().prepend(\u0027\u003Cdiv\u003E\u0027 + \n            \u0027 - Device ID :\u0027+parsedMessage.measureID+\n            \u0027 - Value: \u0027+parsedMessage.value+\n            \u0027 - Latitude: \u0027+parsedMessage.latitude+\n            \u0027 - Longitude: \u0027+parsedMessage.longitude+\n            \u0027\u003C/div\u003E\u0027);\n\n        // Update Line Chart \n        var lineChart = dashboard.getComponentByName(\u0027${c:lineChart}\u0027);\n        lineChart.chart.data.datasets[0].data.shift(); \n        lineChart.chart.data.datasets[0].data.push(parsedMessage.value);\n        lineChart.chart.data.labels.shift();\n        lineChart.data.labels.push(\u0022\u0022);\n        lineChart.chart.update();\n        \n        // Update Bar Chart\n        var barChart = dashboard.getComponentByName(\u0027${c:barChart}\u0027);\n        barChart.chart.data.datasets[0].data[Number(parsedMessage.measureID)-1] = parsedMessage.value;\n        barChart.chart.update();\n        \n        // Update Map Component\n        var map = dashboard.getComponentByName(\u0027${c:map}\u0027);\n        map.mapMarker.setLatLng([Number(parsedMessage.latitude), Number(parsedMessage.longitude)]).update(); \n    });\n\n} "},{"name":"parameter