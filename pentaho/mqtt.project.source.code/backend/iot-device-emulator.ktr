        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.util.*;

import org.eclipse.paho.client.mqttv3.MqttClient;
import org.eclipse.paho.client.mqttv3.MqttConnectOptions;
import org.eclipse.paho.client.mqttv3.MqttException;
import org.eclipse.paho.client.mqttv3.MqttMessage;
import org.eclipse.paho.client.mqttv3.persist.MemoryPersistence;
import org.eclipse.paho.client.mqttv3.persist.MqttDefaultFilePersistence;
import org.eclipse.paho.client.mqttv3.IMqttDeliveryToken;
import org.eclipse.paho.client.mqttv3.MqttCallback;


//private String topic = "pdidatastreamprocessed";
private int outQos = 0;
//private String broker = "tcp://test.mosquitto.org:1883";
//private String broker = "tcp://iot.eclipse.org:1883";
//private String clientId = "pdipubisher";
private MemoryPersistence persistence = new MemoryPersistence();
private FieldHelper inputField = null;

private MqttClient mqttClientProducer;
private MqttConnectOptions connOpts;
private MqttMessage msg;
private String message;


public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] row = getRow();
	String topic = getVariable("topic", "");
	String clientId = getVariable("clientID", "");
	String broker = getVariable("broker", "");
	inputField = get(Fields.In, "message");
	if (row == null) {
        setOutputDone();
		try {
        	mqttClientProducer.disconnect();
			logBasic("Disconnected to: "+broker+" & Topic: "+topic);
		} catch(MqttException me) {
    		logBasic("Reason: "+me.getReasonCode());
        	logBasic("Messgae: "+me.getMessage());
        	logBasic("Loc: "+me.getLocalizedMessage());
        	logBasic("Cause: "+me.getCause());
        	logBasic("Exception: "+me);
        	me.printStackTrace();
			return false;
    	} 
        return false;
    }
	
	if (first){
		first = false;
		try {
			mqttClientProducer = new MqttClient(broker, clientId, persistence);
        	connOpts = new MqttConnectOptions();
        	connOpts.setCleanSession(true);
        	mqttClientProducer.connect(connOpts);
			logBasic("Connected to: "+broker+" & Topic: "+topic);
		} catch(MqttException me) {
    		logBasic("Reason: "+me.getReasonCode());
        	logBasic("Messgae: "+me.getMessage());
        	logBasic("Loc: "+me.getLocalizedMessage());
        	logBasic("Cause: "+me.getCause());
        	logBasic("Exception: "+me);
        	me.printStackTrace();
			return false;
    	} 
	}

	try {
		
		//String message = new String(inputField.getString(row));
		String message = new String(inputField.getString(row));
        MqttMessage msg = new MqttMessage(message.getBytes());
		msg.setQos(outQos);
		mqttClientProducer.publish(topic, msg);
		//logBasic("Publishing message: "+msg);
		//msg = null;
		//message = null;
		//message = null;
	} catch(MqttException me) {
    	logBasic("Reason: "+me.getReasonCode());
        logBasic("Messgae: "+me.getMessage());
        logBasic("Loc: "+me.getLocalizedMessage());
        logBasic("Cause: "+me.getCause());
        logBasic("Exception: "+me);
        me.printStackTrace();
		return false;
    } 


	return true;
}

]]></class_source>
